import { useState, useEffect } from "react";
import axios from "axios";
import config from "../config";
import "./patientcss/MyAppointment.css";

export default function MyAppointment() {
  const [appointments, setAppointments] = useState([]);
  const [error, setError] = useState("");

  useEffect(() => {
    const fetchAppointments = async () => {
      try {
        const patientId = sessionStorage.getItem("patientId");
        if (!patientId) {
          setError("Patient not logged in");
          return;
        }
        const response = await axios.get(`${config.url}/appointments/patient/${patientId}`);
        setAppointments(response.data);
      } catch {
        setError("Failed to fetch appointments. Please try again later.");
      }
    };
    fetchAppointments();
  }, []);

  return (
    <div className="appointments-container">
      <h2 className="appointments-heading">üìã My Appointments</h2>
      {error && <p className="appointments-error">{error}</p>}

      {appointments.length === 0 ? (
        <p className="appointments-empty">No appointments found.</p>
      ) : (
        <div className="appointments-table-container">
          <table className="appointments-table">
            <thead>
              <tr>
                <th>Doctor</th>
                <th>Date</th>
                <th>Time</th>
                <th>Reason</th>
              </tr>
            </thead>
            <tbody>
              {appointments.map((appt, index) => (
                <tr key={index}>
                  <td>{appt.doctorName}</td>
                  <td>{appt.appointmentDate}</td>
                  <td>{appt.appointmentTime}</td>
                  <td>{appt.reason}</td>
                </tr>
              ))}
            </tbody>
          </table>
        </div>
      )}
    </div>
  );
}
