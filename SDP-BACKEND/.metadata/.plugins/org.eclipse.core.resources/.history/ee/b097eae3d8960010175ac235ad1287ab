frontendcodes:
import { useState, useEffect } from "react";
import axios from "axios";
import config from "../config";
import "./patientcss/appointmentBooking.css";

export default function AppointmentBooking() {
  const [formData, setFormData] = useState({
    patientId: "",   // only this is needed
    doctorId: "",
    date: "",
    timeSlot: "",
    reason: ""
  });

  const [doctors, setDoctors] = useState([]);
  const [message, setMessage] = useState("");

  useEffect(() => {
    const patient = JSON.parse(localStorage.getItem("patient")); // from login
    if (patient) {
      setFormData((prev) => ({ ...prev, patientId: patient.id }));
    }

    const fetchDoctors = async () => {
      try {
        const res = await axios.get(`${config.url}/doctor/all`);
        setDoctors(res.data);
      } catch {
        setDoctors([]);
      }
    };
    fetchDoctors();
  }, []);

  const generateTimeSlots = () => {
    let slots = [];
    for (let h = 9; h <= 22; h++) {
      if (h >= 14 && h < 16) continue;
      for (let m of [0, 15, 30, 45]) {
        if (h === 22 && m > 0) continue;
        const time = new Date();
        time.setHours(h, m);
        slots.push(time.toLocaleTimeString([], { hour: "2-digit", minute: "2-digit" }));
      }
    }
    return slots;
  };

  const handleChange = (e) => {
    const { id, value } = e.target;
    setFormData({ ...formData, [id]: value });
  };

  const handleSubmit = async (e) => {
    e.preventDefault();
    try {
      await axios.post(`${config.url}/appointments/book`, formData);
      setMessage("‚úÖ Appointment booked successfully!");
    } catch (err) {
      setMessage("‚ùå Failed to book appointment. Try again.");
    }
  };

  return (
    <div className="appointment-container">
      <h2>üìÖ Book Appointment</h2>
      {message && <p>{message}</p>}

      <form onSubmit={handleSubmit}>
        <label>Doctor</label>
        <select id="doctorId" value={formData.doctorId} onChange={handleChange} required>
          <option value="">-- Choose Doctor --</option>
          {doctors.map((doc) => (
            <option key={doc.id} value={doc.id}>
              {doc.name} ({doc.specialization})
            </option>
          ))}
        </select>

        <label>Date</label>
        <input type="date" id="date" value={formData.date} onChange={handleChange} required />

        <label>Time</label>
        <select id="timeSlot" value={formData.timeSlot} onChange={handleChange} required>
          <option value="">-- Select Time --</option>
          {generateTimeSlots().map((slot, i) => (
            <option key={i} value={slot}>{slot}</option>
          ))}
        </select>

        <label>Reason</label>
        <textarea id="reason" value={formData.reason} onChange={handleChange} required />

        <button type="submit">Book</button>
      </form>
    </div>
  );
}
