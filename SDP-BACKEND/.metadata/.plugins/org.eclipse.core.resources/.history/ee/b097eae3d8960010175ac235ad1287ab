import { useState, useEffect } from "react";
import axios from "axios";
import config from "../config";
import "./doctorcss/ViewAppointment.css";

export default function ViewAppointment() {
  const [appointments, setAppointments] = useState([]);
  const [error, setError] = useState("");

  useEffect(() => {
    const fetchAppointments = async () => {
      try {
        const storedDoctor = JSON.parse(sessionStorage.getItem("doctor"));
        if (!storedDoctor) {
          setError("Doctor not logged in");
          return;
        }
        const response = await axios.get(`${config.url}/appointments/doctor/${storedDoctor.id}`);
        setAppointments(response.data);
      } catch {
        setError("Failed to fetch appointments. Please try again later.");
      }
    };
    fetchAppointments();
  }, []);

  const updateStatus = async (id, status) => {
    const feedback = prompt("Optional feedback (for reschedule/deny):") || "";
    try {
      await axios.put(`${config.url}/appointments/${id}/status?status=${status}&feedback=${feedback}`);
      setAppointments((prev) =>
        prev.map((a) => (a.id === id ? { ...a, status, doctorFeedback: feedback } : a))
      );
    } catch {
      alert("Failed to update status");
    }
  };

  return (
    <div className="appointments-container">
      <h2 className="appointments-heading">üìÖ My Appointments</h2>
      {error && <p className="appointments-error">{error}</p>}

      {appointments.length === 0 ? (
        <p className="appointments-empty">No appointments found.</p>
      ) : (
        <div className="appointments-table-container">
          <table className="appointments-table">
            <thead>
              <tr>
                <th>Patient</th>
                <th>Date</th>
                <th>Time</th>
                <th>Reason</th>
                <th>Contact</th>
                <th>Status</th>
                <th>Feedback</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              {appointments.map((appt) => (
                <tr key={appt.id}>
                  <td>{appt.patientName}</td>
                  <td>{appt.date}</td>
                  <td>{appt.timeSlot}</td>
                  <td>{appt.reason}</td>
                  <td>{appt.patientContact}</td>
                  <td>{appt.status}</td>
                  <td>{appt.doctorFeedback || "-"}</td>
                  <td>
                    <button onClick={() => updateStatus(appt.id, "APPROVED")}>Approve</button>
                    <button onClick={() => updateStatus(appt.id, "DENIED")}>Deny</button>
                    <button onClick={() => updateStatus(appt.id, "RESCHEDULED")}>Reschedule</button>
                  </td>
                </tr>
              ))}
            </tbody>
          </table>
        </div>
      )}
    </div>
  );
}
